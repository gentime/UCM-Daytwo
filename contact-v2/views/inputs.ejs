<!DOCTYPE html>
<html>

<head>
    <link href="bootstrap-combined.min.css" rel="stylesheet" id="bootstrap-css">
     
    <!------ Include the above in your HEAD tag ---------->
</head>

<body>
        <div class="container">
                <table class="table">
                <thead>
                <tr>
                <th scope="col">#</th>
                <th scope="col">First</th>
                <th scope="col">Last</th>
                <th scope="col">Email</th>
                <th scope="col">Subject</th>
                <th scope="col">Message</th>
                </tr>
                </thead>
                <tbody>
                <tr>
                    <%  for(var i=0; i<contacts.length; i++){%>
                
                        <td><%= i+1 %></td>
                
                    <td><%= contacts[i].firstname%></td>
                <td><%= contacts[i].lastname%></td>
                <td><%= contacts[i].email%></td>
                <td><%= contacts[i].subject%></td>
                <td><%= contacts[i].message%></td>



                </tr>
                <% } %>
                </tbody>
                </table>
                </div>
                </body>
                </html>